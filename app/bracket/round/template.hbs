<h2>{{model.round.title}}</h2>
{{#each model.round.matches as |match|}}
  <div class="match">
    <div class="time">
      <p>{{match.time}}</p>
    </div>
    <div class="location">
      <p>{{match.location}}</p>
    </div>
    <div class="matchup">
      {{match-band match=match round=model.round number=1 bands=model.bands}}
      <span>vs.</span>
      {{match-band match=match round=model.round number=2 bands=model.bands}}
    </div>
  </div>
{{/each}}

{{outlet}}
